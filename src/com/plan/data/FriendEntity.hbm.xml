<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.plan.data.FriendEntity" table="Friend" schema="" catalog="Plan">
        <composite-id mapped="true" class="com.plan.data.FriendEntityPK">
            <key-property name="friendAccount" column="friendAccount"/>
            <key-property name="userAccount" column="userAccount"/>
        </composite-id>
    </class>
</hibernate-mapping>