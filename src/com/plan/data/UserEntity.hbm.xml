<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.plan.data.UserEntity" table="User" schema="" catalog="Plan">
        <id name="account">
            <column name="account" sql-type="varchar" length="45" not-null="true"/>
        </id>
        <property name="password">
            <column name="password" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="nickname">
            <column name="nickname" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="avatag">
            <column name="avatag" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="token">
            <column name="token" sql-type="varchar" length="45"/>
        </property>
    </class>
</hibernate-mapping>